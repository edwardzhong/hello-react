{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime",
        [
            "import", { "libraryName": "antd-mobile", "style": "css" }
        ],
        [
            "react-css-modules", {
                "exclude": "node_modules",
                "filetypes": {
                    ".scss": { "syntax": "postcss-scss" }
                },
                "generateScopedName":"[local]___[hash:base64:5]"
            }
        ]           
    ]
}